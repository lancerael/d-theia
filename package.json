{
  "name": "d-theia",
  "version": "3.4.6",
  "description": "dTheia is a JavaScript charting library for visualization of complex data.",
  "homepage": "http://www.dtheia.org",
  "type": "module",
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/lancerael/d-theia.git"
  },
  "author": "Lance Taylor",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:demo": "tsc && vite build --outDir ./docs/demo",
    "docgen": "npx typedoc ./src/index.ts",
    "preview": "pnpm build && vite preview",
    "lint": "npx prettier -c . && npx eslint",
    "lint:fix": "npx prettier -w . && npx eslint --fix",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "dist": "npm run docgen && npm run build:demo"
  },
  "dependencies": {
    "d3-array": "^3.2.3",
    "d3-axis": "^3.0.0",
    "d3-color": "^3.1.0",
    "d3-ease": "^3.0.1",
    "d3-fetch": "^3.0.1",
    "d3-force": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-transition": "^3.0.1"
  },
  "devDependencies": {
    "@pi-lib/config": "^0.4.0",
    "@types/d3-array": "^3.0.5",
    "@types/d3-axis": "^3.0.2",
    "@types/d3-color": "^3.1.0",
    "@types/d3-ease": "^3.0.0",
    "@types/d3-fetch": "^3.0.2",
    "@types/d3-force": "^3.0.4",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-shape": "^3.1.1",
    "@types/d3-transition": "^3.0.3",
    "@types/jest": "^29.5.1",
    "@types/jest-axe": "^3.5.5",
    "@vitest/coverage-istanbul": "^0.31.1",
    "cross-env": "7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-axe": "^7.0.1",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "rollup": "^3.23.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.24.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.1"
  }
}
