{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "rules": {
    "semi": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "no-plusplus": 0,
    "radix": 0
  }
}
